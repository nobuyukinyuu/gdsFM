func _ready():
	pass



func _on_Slider_value_changed(value):
#	var val =  54.0 / pow(2, value-1)
#	$\"../Label2\".text = String(val)

	
	var val = Glue.EaseFast(0.25, value)
	var val2 = ease(0.25, value)
	$\"../Label2\".text = \"%s: %s, %s = %s%%\" % [value, val, val2, (val-val2)*100]
